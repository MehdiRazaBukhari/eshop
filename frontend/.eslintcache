olume/Practice/React/Practice_Projects/e_shop/frontend/src/Redux/Actions/loginUser.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/Redux/Reducers/userLoginReducer.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/components/Message/Message.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/components/Loading/Loading.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/Redux/Actions/logoutUser.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/screens/RegisterScreen/RegisterScreen.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/Redux/Actions/registerUser.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/Redux/Reducers/registerUser.js",[],"/media/xyz/New Volume/Practice/React/Practice_Projects/e_shop/frontend/src/screens/ProfileScreen/ProfileScreen.js",["215"],"import Loading from '../../components/Loading/Loading'\nimport Message from '../../components/Message/Message'\nimport { useEffect, useState } from 'react'\nimport { Row, Col, Form, Button } from 'react-bootstrap'\nimport { useDispatch, useSelector } from 'react-redux'\nimport getProfile from '../../Redux/Actions/getProfile'\nimport updateUserProfile from '../../Redux/Actions/updateUserProfile'\nimport deleteStoredProfile from '../../Redux/Actions/deleteStoredProfile'\n\nconst ProfileScreen = ({ history }) => {\n  const [name, setName] = useState('')\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n  const [confirmPassword, setConfirmPassword] = useState('')\n  const [passwordError, setPasswordError] = useState('')\n\n  const { loading, profile, error } = useSelector((state) => state.profile)\n  const { update_loading, update_success, update_error } = useSelector(\n    (state) => state.profile_update\n  )\n\n  const { user } = useSelector((state) => state.loggedUser)\n  const dispatch = useDispatch()\n  const handleName = (e) => {\n    setName(e.target.value)\n  }\n\n  const handleEmail = (e) => {\n    setEmail(e.target.value)\n  }\n\n  const handlePassword = (e) => {\n    setPassword(e.target.value)\n  }\n\n  const handleConfirmPassword = (e) => {\n    setConfirmPassword(e.target.value)\n  }\n\n  const handleSubmit = (e) => {\n    e.preventDefault()\n\n    if (password !== confirmPassword) {\n      setPasswordError('Password fields must match!')\n    } else {\n      setPasswordError('')\n\n      if (user && user.token) {\n        let newData\n        if (name.length && email.length && password.length) {\n          newData = { name, email, password }\n        } else if (name.length && email.length) {\n          newData = { name, email }\n        } else if (name.length && password.length) {\n          newData = { name, password }\n        } else if (email.length && password.length) {\n          newData = { email, password }\n        } else if (name.length) {\n          newData = { name }\n        } else if (email.length) {\n          newData = { email }\n        } else if (password.length) {\n          newData(password)\n        }\n\n        dispatch(updateUserProfile(newData))\n      } else {\n        history.push('/login?redirect=profile')\n      }\n    }\n  }\n\n  useEffect(() => {\n    if (user && user.token && profile) {\n      setName(profile.name)\n      setEmail(profile.email)\n    }\n  }, [profile, user])\n\n  useEffect(() => {\n    if (user && user.token && !profile) {\n      dispatch(getProfile())\n    } else if (!user || !user.token) {\n      history.push('/login?redirect=profile')\n    }\n  }, [history, dispatch, user, profile])\n  return (\n    <>\n      <Row className='mt-5'>\n        <Col sm={12} md={5} l={5} xl={5}>\n          {loading ? (\n            <Loading />\n          ) : error ? (\n            <Message variant='danger'>{error}</Message>\n          ) : (\n            <>\n              {update_success ? (\n                <Message variant='success'>Profile updated successfuly</Message>\n              ) : null}\n              {update_loading ? <Loading type='ThreeDots' height='20' /> : null}\n              {update_error ? (\n                <Message variant='danger'>{update_error}</Message>\n              ) : null}\n              {passwordError.length ? (\n                <Message variant='danger'>{passwordError}</Message>\n              ) : null}\n              <Form>\n                <Form.Group>\n                  <Form.Label>Name</Form.Label>\n                  <Form.Control\n                    type='name'\n                    as='input'\n                    value={name}\n                    onChange={handleName}\n                  ></Form.Control>\n                </Form.Group>\n\n                <Form.Group>\n                  <Form.Label>Email</Form.Label>\n                  <Form.Control\n                    type='email'\n                    as='input'\n                    value={email}\n                    onChange={handleEmail}\n                  ></Form.Control>\n                </Form.Group>\n\n                <Form.Group>\n                  <Form.Label>Password</Form.Label>\n                  <Form.Control\n                    type='password'\n                    as='input'\n                    value={password}\n                    onChange={handlePassword}\n                  ></Form.Control>\n                </Form.Group>\n\n                <Form.Group>\n                  <Form.Label>Confirm Password</Form.Label>\n                  <Form.Control\n                    type='password'\n                    as='input'\n                    value={confirmPassword}\n                    onChange={handleConfirmPassword}\n                  ></Form.Control>\n                </Form.Group>\n\n                <div className='text-right'>\n                  <Button type='submit' onClick={handleSubmit}>\n                    Update\n               